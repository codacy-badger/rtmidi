prefix=@prefix@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include/rtmidi@rtmidi_suffix@

Name: librtmidi
Description: RtMidi - a set of C++ classes that provide a common API for realtime MIDI input/output
Version: @PACKAGE_VERSION@
Requires: @req@ 
Libs: -L${libdir} -lrtmidi@rtmidi_suffix@
Libs.private: -lpthread
Cflags: -pthread -I${includedir} @CPPFLAGS@